<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Gentelella Alela! | </title>

    <link href="@Url.Content("~/Content/")css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Content/")font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Content/")plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->

    <link href="@Url.Content("~/Content/")plugins/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="@Url.Content("~/Content/")plugins/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- Datatables -->

    <link href="@Url.Content("~/Content/")DataTable/datatables/dataTables.bootstrap.css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/")plugins/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="@Url.Content("~/Content/")css/datepicker.css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/")build/css/custom.min.css" rel="stylesheet" />
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Dashboard</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            @*@* <img src="@Url.Content("~/Content/")images/img.jpg" alt="..." class="img-circle profile_img">*@
                            @*<img src="@Url.Content("")"*@*

                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2>Black Shadow Team</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!--====== sidebar menu =============-->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li><a href="#"><i class="fa fa-home"></i> Home</a></li>
                                <li>
                                    <a><i class="fa fa-edit"></i> Setup <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li> @Html.ActionLink("Department Setup", "Department", "Department")</li>     
                                        <li> @Html.ActionLink("Course Setup", "Course", "Course")</li>   
                                        <li> @Html.ActionLink("AllocatedClassRoom", "AllocatedClassRoom", "AllocatedClassRoom")</li>                                
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart"></i> Teacher Info <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li>@Html.ActionLink("Teacher Setup", "Teacher", "Teacher")</li>    
                                        <li> @Html.ActionLink("Course Assign to Teacher", "CourseAssign", "CourseAssign")</li>                                      
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart"></i> Class Room Info<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li>@Html.ActionLink("Class Room Allocate", "AllocatedClassRoom", "AllocatedClassRoom")</li>
                                        <li> @Html.ActionLink("View ClassRoom ", "ViewClassSchdule", "ViewClassSchdule")</li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-table"></i> Student Info <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li>@Html.ActionLink("Register Student", "RegisterStudent", "RegisterStudent")</li> 
                                        <li>@Html.ActionLink("Enroll in a Course", "EnrollCourse", "EnrollCourse")</li>   
                                        
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart-o"></i>Student Result info<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li>@Html.ActionLink("Student Result","StudentResult","StudentResult")</li>
                                        <li>@Html.ActionLink("View Result", "ViewResult", "ViewResult")</li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart-o"></i>Unassign Course & Class<span class="fa fa-chevron-down"></span></a>
                                     <ul class="nav child_menu">
                                        <li>@Html.ActionLink("Unassign Course", "UnassignCourse", "UnassignCourse")</li>
                                        <li>@Html.ActionLink("Unassign ClassRoom", "ViewResult", "ViewResult")</li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!--====== End sidebar menu =============-->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <!-- <a data-toggle="tooltip" data-placement="top" title="Settings">
                          <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                          <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                          <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout" href="login.html">
                          <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>-->
                        <span style="float:left;width:100%; height:30px;"></span>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <div class="site-title">University Course & Result Management System</div>
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false" style="color:#FFF !important">
                                    <img src="@Url.Content("~/Content/")images/img.jpg" alt="">Black Shadow
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                    <li><a href="javascript:;"> Profile</a></li>
                                    <li>
                                        <a href="javascript:;">
                                            <span class="badge bg-red pull-right">50%</span>
                                            <span>Settings</span>
                                        </a>
                                    </li>
                                    <li><a href="javascript:;">Help</a></li>
                                    <li><a href="login.html"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="dashboard_graph">
                            @RenderBody()
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div><!--===== End Row ====-->
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    CopyRight &copy; 2017 | all rights reserved | Design and Developed by <a href="https://fb.com">RB</a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>
    <!-- Bootstrap -->
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/")plugins/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- jQuery -->
    <script src="@Url.Content("~/Content/")plugins/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="@Url.Content("~/Content/")plugins/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/")plugins/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="@Url.Content("~/Content/")plugins/nprogress/nprogress.js"></script>
    <!-- iCheck -->
    <script src="@Url.Content("~/Content/")plugins/iCheck/icheck.min.js"></script>

    <!-- Datatables -->

    <script src="@Url.Content("~/Content/")DataTable/datatables/jquery.dataTables.min.js"></script>
    <script src="@Url.Content("~/Content/")DataTable/datatables/dataTables.bootstrap.min.js"></script>

    <script>
        $(function () {
            $("#datatable").DataTable(); // add more datatable changin only id name
        });
    </script>

    <script src="@Url.Content("~/Content/")plugins/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="@Url.Content("~/Content/")js/datepicker.js"></script>
    <script>
        $(function () {
            $('#datepicker').datepicker({ dateFormat: 'dd/mm/yy' }).val();
        });
    </script>
    <!-- Custom Theme Scripts -->
    <script src="@Url.Content("~/Content/")build/js/custom.min.js"></script>

